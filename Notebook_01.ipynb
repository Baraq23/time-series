   "source": [
    "import pandas as pd\n",
    "\n",
    "dates = pd.date_range(start='2010-01-01', end='2020-12-31')\n",
    "\n",
    "integer_series = pd.Series((dates - pd.Timestamp('2010-01-01')).days, index=dates)\n",
    "\n",
    "moving_avg = integer_series.rolling(window=7).mean()\n",
    "\n",
    "print(moving_avg.head(10))"
   ]
